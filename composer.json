{
    "name": "mamba/base",
    "description": "Mamba Framework Base Parts",
    "keywords": ["silex", "microframework"],
    "license": "MIT",
    "version": "1.1.0",
    "authors": [
        {
            "name": "Mauro Cassani",
            "email": "assistenza@easy-grafica.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "container-interop/container-interop": "^1.1",
        "danielstjules/stringy": "~2.3",
        "doctrine/dbal": "~2.2",
        "doctrine/migrations": "^1.4",
        "dflydev/doctrine-orm-service-provider": "^2.0",
        "guzzlehttp/guzzle": "^6.2",
        "knplabs/console-service-provider": "^2.0",
        "yosymfony/config-loader": "1.*@dev",
        "memio/memio": "^1.0",
        "monolog/monolog": "^1.21",
        "ocramius/proxy-manager": "^1.0",
        "silex/web-profiler": "^2.0",
        "silex/silex": "~2.0",
        "swiftmailer/swiftmailer": "^5.4",
        "symfony/asset": "^3.1",
        "symfony/config": "^3.1",
        "symfony/form": "^3.1",
        "symfony/intl": "^3.1",
        "symfony/monolog-bridge": "^3.1",
        "symfony/security-csrf": "^3.1",
        "symfony/serializer": "^3.1",
        "symfony/translation": "^3.1",
        "symfony/twig-bridge": "^3.1",
        "symfony/validator": "^3.1",
        "symfony/yaml": "^3.1",
        "twig/twig": "^1.24",
        "twig/extensions": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1",
        "phpunit/php-code-coverage": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Mamba\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
